{
  "name": "digitaldonkey/ethereum-php",
  "description": "A library integrating Ethereum with typed PHP.",
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "graze/guzzle-jsonrpc": "~3.0",

    "pear/math_biginteger": "^1.0",
    "kornrunner/keccak": "^1.0",
    "digitaldonkey/ecverify":"1.0",
    "react/event-loop": "^1.0",
    "react/stream": "^1.0",
    "everdreamsoft/crystal-spark-cannon": "dev-master"
  },
  "scripts": {
    "generate-methods": [
      "php scripts/generate-methods.php"
    ],
    "generate-complex-datatypes": [
      "php scripts/generate-complex-datatypes.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0 || ^5.0",
    "gossi/php-code-generator": "^0.5.0",

    "everdreamsoft/sandra": "dev-master"
  },


  "autoload": {
    "psr-4": {
      "Ethereum\\": "src/"
    }
  },


  "minimum-stability": "dev",
  "prefer-stable": true,


  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Krug",
      "email": "thorsten.krug@consensys.net"
    },
    {
      "name": "Shaban Shaame",
      "email": "shaban@everdreamsoft.com"
    }
  ]
}
